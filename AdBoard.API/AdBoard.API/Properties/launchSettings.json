{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "debug": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:8081",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "release": {
      "commandName": "Project",
      "launchBrowser": false,
      "applicationUrl": "http://localhost:8081",
      "environmentVariables": {
        "ASPNETCODE_ENVIRONMENT": "Production"
      }
    }

  }
}
